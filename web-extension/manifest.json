{
  "manifest_version": 3,
  "name": "Page-HTML-Ripper",
  "version": "1.0",
  "description": "Download the full HTML of the current page.",
  "icons": {
    "48": "icons/icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "downloads",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
